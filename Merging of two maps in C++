#include <iostream>
#include <map>
using namespace std;

int main()
{
    map<int, string> map1{ { 10, "diya" }, { 20, "riya" } };
    map<int, string> map2{ { 30, "deep" }, { 40, "sameer" } };
    map1.insert(map2.begin(), map2.end());
    cout << "Concatenated Map:{";
    for (auto pair : map1) {
        cout << "{" << pair.first << ": " << pair.second
             << "}";
    }
    cout << " }" << endl;
    return 0;
}
