#include <iostream>
using namespace std;
template <typename T1, typename T2 = double, typename T3 = string> class Funct
{
  public:
    T1 x;
    T2 y;
    T3 z;
    Funct(T1 val1, T2 val2, T3 val3) : x(val1), y(val2), z(val3)
    {
    }
    void getValues()
    {
        cout << x << " " << y << " " << z;
    }
};

int main()
{
    Functk<int, float, string> intFloatStringFunct(10, 5.67f, "Hello");
    Funct<char> charDoubleStringFunct('A', 3.14, "World");
  intFloatStringGeek.getValues();
    cout << endl;
    charDoubleStringFunct.getValues();
return 0;
}
