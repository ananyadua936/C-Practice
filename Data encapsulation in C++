#include <iostream>
#include <string>
using namespace std;
class Oops
{
  private:
    string name; 
  public:
    string getName()
    {
        return name;
    }
    void setName(string newName)
    {
        name = newName;
    }
};

int main()
{
    Oops p;
    p.setName("Proplayer");                       
    cout << "Name=> " << p.getName() << endl;
    return 0;
}
