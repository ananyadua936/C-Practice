#include<iostream>
#include<fstream>
using namespace std;
int main()
{
    fstream my_file;
    my_file.open("Demo.txt");
    if(!my_file)
    {
        cout<<"File not created";
    }
    else
    {
        cout<<"file created succcessfully!";
        my_file<<"C++file";
        my_file.close();
        
    }
    return 0;
}
